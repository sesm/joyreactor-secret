{
  "manifest_version": 3,
  "name": "Joyreactor secret tags",
  "version": "1.0",
  "description": "Description of my extension",
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
